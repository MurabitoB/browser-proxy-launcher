{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Browser Proxy Launcher",
  "version": "0.1.3",
  "identifier": "com.browser-proxy-launcher.app",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../out",
    "removeUnusedCommands": true
  },
  "app": {
    "windows": [
      {
        "title": "Browser Proxy Launcher",
        "width": 600,
        "height": 900,
        "minWidth": 600,
        "minHeight": 900,
        "visible": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
